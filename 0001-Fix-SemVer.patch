index 0000000..a3b0dd5
--- /dev/null
+++ b/commit_id
@@ -0,0 +1 @@
+b190389914371a0439492471444585b2feb52903
diff --git a/configure.ac b/configure.ac
index 956c1e8..e2bf7ef 100644
--- a/configure.ac
+++ b/configure.ac
@@ -21,7 +21,7 @@
 #
 
 AC_PREREQ([2.69])
-AC_INIT([cc-oci-runtime], [0.1])
+AC_INIT([cc-oci-runtime], [2.0])
 AC_CONFIG_MACRO_DIR([m4])
 AM_INIT_AUTOMAKE([foreign -Wall -Werror -Wno-portability silent-rules subdir-objects color-tests no-dist-gzip dist-xz])
 AM_SILENT_RULES([yes])
